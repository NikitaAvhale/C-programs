#include<iostream.h>
#include<conio.h>
class sum_numbers
{
int n;
int sum;
public:
class sum_numbers(int num)
{
n=num;
sum=0;
for(int i=1; i<=n; i++)
{
sum +=i;
}
}
void display()
{
cout<<"sum of numbers from"<<n<<"is"<<sum<<endl;
}
};
